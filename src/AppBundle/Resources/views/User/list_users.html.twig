<!doctype html>
<html>
	<head>
		<meta charset="utf-8">
		<link rel="stylesheet" href="{{ asset('bundles/app/css/style.css') }}"/>
		<style>
			td {
				border: solid black 1px;
			}
		</style>
	</head>
	<body>
		<table>
			<thead>
				<tr>
					<th>firstName</th>
					<th>lastName</th>
					<th>phoneNumbers</th>
					<th>Age</th>
				</tr>
			</thead>
			<tbody>
				{% for user in users %}
					<tr>
						<td>{{ user.firstName }}</td>
						<td>{{ user.lastName }}</td>
						<td>
							{% for phoneNumber in user.phoneNumbers %}
								<br/>{{ phoneNumber.number }}
							{% endfor %}
						</td>
						<td>{{ user.age }}</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>
	</body>
</html>